[odr_pub_app_logs]
log_group_name = odr-pub_logs
log_stream_name = odr-pub_{ip_address}_app.log
file = /var/log/nodejs/app/appOBP.log*
[odr_pub_nginx_access]
log_group_name = odr-pub_logs
log_stream_name = odr-pub_{ip_address}_nginx_access.log
file = /var/log/nginx/access.log
[odr_pub_nginx_error]
log_group_name = odr-pub_logs
log_stream_name = odr-pub_{ip_address}_nginx_error.log
file = /var/log/nginx/error.log