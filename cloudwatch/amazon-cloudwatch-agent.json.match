      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/match/m_*.log",
            "log_group_name": "match0_logs",
            "log_stream_name": "match0_{ip_address}_m_match_logs",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/match/c_*.log",
            "log_group_name": "match0_logs",
            "log_stream_name": "match0_{ip_address}_c_match_logs",
