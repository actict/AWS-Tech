{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-2",
    "logfile": "",
    "debug": false
  },

  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/home/ec2-user/.pm2/logs/*.log",
            "log_group_name": "admin-access_logs",
            "log_stream_name": "admin-access_{ip_address}_PM2.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/home/ec2-user/admin-access/log/*.log",
            "log_group_name": "admin-access_logs",
            "log_stream_name": "admin-access_{ip_address}_app.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/access.log",
            "log_group_name": "admin-access_logs",
            "log_stream_name": "admin-access_{ip_address}_nginx_access.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/error.log",
            "log_group_name": "admin-access_logs",
            "log_stream_name": "admin-access_{ip_address}_nginx_error.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          }
        ]
      }
    },
    "log_stream_name":"admin-access_log_default"
  }
}
