[odr_svr_app_logs]
log_group_name = odr-svr_logs
log_stream_name = odr-svr_{ip_address}_app.log
file = /var/log/nodejs/app/appOBS.log*
[odr_svr_nginx_access]
log_group_name = odr-svr_logs
log_stream_name = odr-svr_{ip_address}_nginx_access.log
file = /var/log/nginx/access.log
[odr_svr_nginx_error]
log_group_name = odr-svr_logs
log_stream_name = odr-svr_{ip_address}_nginx_error.log
file = /var/log/nginx/error.log