{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-2",
    "logfile": "",
    "debug": false
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/syslog",
            "log_group_name": "candy_deploy_logs",
            "log_stream_name": "candy_deploy_{ip_address}_syslog",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/jenkins/jenkins.log",
            "log_group_name": "candy_deploy_logs",
            "log_stream_name": "candy_deploy_{ip_address}_jenkins.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/access.log",
            "log_group_name": "candy_deploy_logs",
            "log_stream_name": "candy_deploy_{ip_address}_access.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/error.log",
            "log_group_name": "candy_deploy_logs",
            "log_stream_name": "candy_deploy_{ip_address}_error.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          }
        ]
      }
    },
    "log_stream_name":"candy_deploy_log_default"
  }
}
