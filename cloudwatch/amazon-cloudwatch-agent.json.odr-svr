{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-2",
    "logfile": "",
    "debug": false
  },

  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/nodejs/app/appOBS*",
            "log_group_name": "odr-svr_logs",
            "log_stream_name": "odr-svr_{ip_address}_log",
            "timezone": "UTC",
            "encoding": "utf-8"
          }
        ]
      }
    },
    "log_stream_name":"odr-svr_log_default"
  }
}
