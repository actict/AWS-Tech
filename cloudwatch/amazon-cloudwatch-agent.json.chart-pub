{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-2",
    "logfile": "",
    "debug": false
  },

  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/nodejs/app/appCHP*",
            "log_group_name": "chart-pub_logs",
            "log_stream_name": "chart-pub_{ip_address}_log",
            "timezone": "UTC",
            "encoding": "utf-8"
          }
        ]
      }
    },
    "log_stream_name":"chart-pub_log_default"
  }
}
