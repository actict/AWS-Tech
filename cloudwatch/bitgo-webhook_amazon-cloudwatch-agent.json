{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "ap-northeast-2",
    "logfile": "",
    "debug": false
  },

  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/home/ec2-user/.pm2/logs/*.log",
            "log_group_name": "bitgo_webhook_logs",
            "log_stream_name": "bitgo_webhook_{ip_address}_PM2.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/access.log",
            "log_group_name": "bitgo_webhook_logs",
            "log_stream_name": "bitgo_webhook_{ip_address}_access.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/var/log/nginx/error.log",
            "log_group_name": "bitgo_webhook_logs",
            "log_stream_name": "bitgo_webhook_{ip_address}_error.log",
            "timezone": "UTC",
            "encoding": "utf-8"
          },
          {
            "file_path": "/log/bitgo/bitgo.log.*",
            "log_group_name": "bitgo_webhook_logs",
            "log_stream_name": "bitgo_webhook_{ip_address}_log",
            "timezone": "UTC",
            "encoding": "utf-8"
          }
        ]
      }
    },
    "log_stream_name":"bitgo_webhook_log_default"
  }
}
